{
  "sourceMaps": "both",
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "loose": true,
        "useBuiltIns": "entry",
        "shippedProposals": true
      }
    ],
    "@babel/react",
    "@babel/stage-3"
  ],
  "plugins": [
    "@babel/transform-runtime"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-inline-environment-variables"
      ],
      "presets": [
        "react-optimize"
      ]
    }
  }
}