{
  "typescript.tsdk": "./node_modules/typescript/lib",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "build/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "build/": true,
    "src/assets": true
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true
}
