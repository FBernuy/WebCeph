    "tslint": "tslint 'src/**/*.ts{,x}' -p ./src",
    "eslint": "eslint '**/*.js' src",
    "lint": "npm run tslint && npm run eslint",
    "test": "jest",
    "dev": "NODE_ENV=development HOT=1 webpack-dev-server --watch",
    "prod": "NODE_ENV=production webpack --bail --progress"
  },
