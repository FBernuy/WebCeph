  "bundlesize": [
    {
      "path": "./build/**/!(vendor).*.js",
      "maxSize": "15 kB"
    },
    {
      "path": "./build/**/vendor.*.js",
      "maxSize": "130 kB"
    },
    {
      "path": "./build/**/*.module.*.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./build/**/*.global.*.css",
      "maxSize": "10 kB"
    },
    {
      "path": "./build/**/*.html",
      "maxSize": "3 kB"
    },
    {
      "path": "./build/**/*.svg",
      "maxSize": "30 kB"
    }
  ],
  "lint-staged": {
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.{j,t}s{x,}": [
      "prettier --write",
      "git add"
    ],
    "**/!(package).json": [
      "prettier --write",
      "git add"
    ],
    "**/*.js{x,}": [
      "eslint"
    ],
    "**/**/*.{s,}css": [
      "stylelint --syntax scss",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "pre-push": "yarn pretest",
      "post-merge": "yarnhook",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "browserslist": [
    "and_chr >= 62",
    "chrome >= 55",
    "edge >= 14",
    "firefox >= 57",
    "ios_saf >= 9.3",
    "safari >= 9.1"
  ],
  "engines": {
    "node": ">= 6.10"
  }
}
